; *************************************
; BEGIN OF RUSSIAN KEYBOARD HANDLER
; *************************************
;
I0DA5:  DEFB	030H,031H,032H,033H,034H,035H,036H,037
        DEFB	038H,039H,02DH,03DH,05CH,05BH,05DH,03B
        DEFB	027H,060H,02CH,02EH,02FH,02CH,061H,062
        DEFB	063H,064H,065H,066H,067H,068H,069H,06A
        DEFB	06BH,06CH,06DH,06EH,06FH,070H,071H,072
        DEFB	073H,074H,075H,076H,077H,078H,079H,07A

        DEFB	029H,021H,040H,023H,024H,025H,05EH,026
        DEFB	02AH,028H,05FH,02BH,07CH,07BH,07DH,03A
        DEFB	022H,07EH,03CH,03EH,03FH,02EH,041H,042
        DEFB	043H,044H,045H,046H,047H,048H,049H,04A
        DEFB	04BH,04CH,04DH,04EH,04FH,050H,051H,052
        DEFB	053H,054H,055H,056H,057H,058H,059H,05A

        DEFB	009H,0B4H,0BDH,0BCH,0B7H,0ADH,0AFH,0BB
        DEFB	0ACH,007H,0B0H,01FH,01EH,090H,08FH,09A
        DEFB	0A7H,001H,0B3H,0B2H,01DH,00DH,0A0H,096
        DEFB	08DH,08EH,082H,0ABH,0A9H,0A6H,085H,0A3
        DEFB	0A5H,097H,094H,092H,09FH,083H,080H,09B
        DEFB	0A1H,093H,095H,0B1H,09CH,01CH,091H,087

        DEFB	00AH,0B5H,003H,004H,005H,006H,084H,00B
        DEFB	00CH,008H,017H,0B1H,016H,03CH,03EH,0AA
        DEFB	000H,002H,0BEH,0BFH,0B6H,00EH,00FH,01A
        DEFB	0AEH,0A2H,088H,098H,014H,015H,08AH,013
        DEFB	0A4H,0A8H,01BH,011H,09EH,089H,086H,0B8
        DEFB	010H,018H,019H,08CH,09DH,0BAH,012H,08B

        DEFB	030H,031H,032H,033H,034H,035H,036H,037
        DEFB	038H,039H,02DH,03DH,02FH,0C8H,0DFH,0D6
        DEFB	0DCH,017H,0C2H,0C0H,099H,02CH,0C6H,0C9
        DEFB	0D3H,0D7H,0D5H,0C1H,0D0H,0D2H,0DBH,0CF
        DEFB	0CCH,0C4H,0D8H,0D4H,0DDH,0DAH,0CAH,0CB
        DEFB	0D9H,0C5H,0C7H,0CDH,0C3H,0DEH,0CEH,0D1

        DEFB	029H,021H,022H,023H,03BH,025H,03AH,03F
        DEFB	02AH,028H,05FH,02BH,05CH,0E8H,0FFH,0F6
        DEFB	0FCH,010H,0E2H,0E0H,0B9H,02EH,0E6H,0E9
        DEFB	0F3H,0F7H,0F5H,0E1H,0F0H,0F2H,0FBH,0EF
        DEFB	0ECH,0E4H,0F8H,0F4H,0FDH,0FAH,0EAH,0EB
        DEFB	0F9H,0E5H,0E7H,0EDH,0E3H,0FEH,0EEH,0F1


J0EC5:	LD	E,C
        LD	D,00H
        LD	HL,FNKFLG-035H
        ADD	HL,DE
        LD	A,(HL)
        AND	A
        JR	NZ,J0EE3
J0ED0:	EX	DE,HL
        ADD	HL,HL
        ADD	HL,HL
        ADD	HL,HL
        ADD	HL,HL
        LD	DE,FNKSTR-035H*16
        ADD	HL,DE
        EX	DE,HL
J0EDA:	LD	A,(DE)
        AND	A
        RET	Z
        CALL	C0F55
        INC	DE
        JR	J0EDA

J0EE3:	LD	HL,(CURLIN)
        INC	HL
        LD	A,H
        OR	L
        JR	Z,J0ED0
        LD	HL,TRPTBL-035H*3
        ADD	HL,DE
        ADD	HL,DE
        ADD	HL,DE

;	Subroutine	raise trap
;	Inputs		________________________
;	Outputs		________________________
;	Remark		code identical among keyboard layout versions

C0EF1:	LD	A,(HL)
        AND	01H
        RET	Z
        LD	A,(HL)
        OR	04H
        CP	(HL)
        RET	Z
        LD	(HL),A
        XOR	05H
        RET	NZ
        LD	A,(ONGSBF)
        INC	A
        LD	(ONGSBF),A
        RET

J0F06:	LD	A,(NEWKEY+6)
        RRCA
        LD	A,0CH
        SBC	A,0
        JR	C0F55

;	  Subroutine key handler for scancode 03AH and above returning normal keycode
;	     Inputs  ________________________
;	     Outputs ________________________

J0F10:	LD	A,3CH		; SHIFT,CTRL,GRAPH : A points to zero value in the table
J0F12:	CALL	H.KEYA		; ESC,TAB,BS,SELECT,RETURN,SPACE,ins,del,left,up,down,right,numeric pad handler
        LD	E,A
        LD	D,00H
        LD	HL,D1003-03AH
        ADD	HL,DE
        LD	A,(HL)
        AND	A
        RET	Z			; no action, quit
        JR	C0F55

;	  Subroutine russian �kana� key
;	     Inputs  ________________________
;	     Outputs ________________________

J0F21:	XOR A	; KANA (LAT) handler
J0F22:  LD	HL,KANAST
        LD	(HL),A
        LD	A,0FH
        OUT	(0A0H),A
        IN	A,(0A2H)
        AND	7FH
        LD	B,A
        LD	A,(HL)
        AND	80H
        LD	C, A	; From here it differs from standard one, 2 bytes economy.
        OR	B
        OUT	(0A1H),A	; Bit 7 is used as Russian flag
        LD	A, C
        RLCA
        XOR, 0DH
        OUT (0ABH),A
        RET
K.BCAP:	RET		; 0F3D: No action to do, CAPS LED used to indicate RUS

;	  Subroutine CAPS key handler
;	     Inputs  ________________________
;	     Outputs ________________________

        DEFB 00H, 00H, 00H, 00H; unused space (4 bytes)

J0F42:	LD	A, 0FFH		; CAPS (RUS) handler
        JR J0F22


J0F46:	LD	A,(NEWKEY+6)
        RRCA	
        RRCA	
        LD	A,03H
        JR	NC,J0F50
        INC	A
J0F50:	LD	(INTFLG),A
        JR	C,J0F64

;	  Subroutine __________________________
;	     Inputs  ________________________
;	     Outputs ________________________

C0F55:	LD	HL,(PUTPNT)
        LD	(HL),A
        CALL	C10C2
        LD	A,(GETPNT)
        CP	L
        RET	Z
        LD	(PUTPNT),HL
J0F64:	LD	A,(CLIKSW)
        AND	A
        RET	Z
        LD	A,(CLIKFL)
        AND	A
        RET	NZ
        LD	A,0FH
        LD	(CLIKFL),A
        OUT	(0ABH),A
        LD	A,0AH
J0F77:	DEC	A
        JR	NZ,J0F77

K.BSND:	AND	A
        LD	A,0EH
        JR	Z,J0F80
        INC	A
J0F80:	OUT	(0ABH),A
        RET	

;	  Subroutine ordinary key handler
;	     Inputs  ________________________
;	     Outputs ________________________

J0F83:	LD	A,(NEWKEY+6)
        LD	E,A
        RRA	
        RRA	
        PUSH	AF
        LD	A,E
        CPL	
        JR	NC,J0FA2		; CTRL pressed,
        RRA	
        RRA	
        RLCA	
        AND	03H	; 3 
        BIT	1,A
        JR	NZ,J0FA4		; GRAPH pressed,
        PUSH	AF
        LD	A,(KANAST)
        AND	04H			; Bit 2 is used as Russian flag
        LD	E,A
        POP	AF
        OR	E
        JR	J0FA4

J0FA2:	AND	001H			; SHIFT state
J0FA4:	LD	E,A				; If jumped here, shift state may contain 2*graph and 4*kana
        ADD	A,A
        ADD	A,E
        ADD	A,A
        ADD	A,A
        ADD	A,A
        ADD	A,A
        LD	E,A				; SHIFT state * 48
        LD	D,00H
        LD	HL,I0DA5
        ADD	HL,DE			; pointer to one of 6 tables
        LD	B,D
        ADD	HL,BC			; pointer to character
        POP	AF
        LD	A,(HL)
        JR	C,J0FCE			; CTRL not pressed,
        AND	0DFH
        SUB	40H
        CP	20H
        RET	NC
J0FBF:	JR	C0F55

;	  Subroutine function key handler
;	     Inputs  ________________________
;	     Outputs ________________________

J0FC1:	LD	A,(NEWKEY+6)
        RRCA	
        JR	C,J0FCB
        LD	A,C
        ADD	A,05H
        LD	C,A
J0FCB:	JP	J0EC5

J0FCE:	CP	20H
        JR	NC,J0FDD
        PUSH	AF
        LD	A,01H
        CALL	C0F55
        POP	AF
        ADD	A,40H
        JR	J0FBF

J0FDD:	LD	HL,CAPST
        INC	(HL)
        DEC	(HL)
        JR	Z,J0FFE
        CP	099H	; ё
        JR	Z,RU_UPC;
        CP	61H
        JR	C,J0FFE
        CP	0E0H
        JR	NC,J0FFE
        CP	7BH
        JR	C,J0FFC
        CP	0C0H
        JR	C,J0FFE
RU_UPC: OR	20H
        JR	J0FFE

J0FFC:	AND	0DFH
J0FFE:	JR	J0FBF

        DB 0, 0, 0		; unused space (3 bytes)

D1003:	DEFB	        1BH,09H,00H,08H,18H,0DH
        DEFB	20H,0CH,12H,7FH,1DH,1EH,1FH,1CH
        DEFB	0DH,2BH,2AH,30H,31H,32H,33H,34H
        DEFB	35H,36H,37H,38H,39H,2DH,2CH,2EH

K.HAND:	LD	A,C		; at 1021H
        LD	HL,I1B96
        CALL	H.KEYC
        LD	D,0FH
J102A:	CP	(HL)
        INC	HL
        LD	E,(HL)
        INC	HL
        PUSH	DE
        RET	C
        POP	DE
        JR	J102A


;	  Subroutine __________________________
;	     Inputs  ________________________
;	     Outputs ________________________

D1033:	; was handler table here, moved to D1003

        DEFS	01073H-$,0		; unused space (143 bytes)

        ORG	01B94H

        JR	J1BAC

I1B96:	DEFB	030H,J0F83 AND 255		; scancodes 000H-02FH
        DEFB	033H,J0F10 AND 255		; SHIFT,CTRL,GRAPH
        DEFB	034H,J0F42 AND 255		; CAPS (RUS)
        DEFB	035H,J0F21 AND 255		; CODE (LAT)
        DEFB	03AH,J0FC1 AND 255		; F1,F2,F3,F4,F5
        DEFB	03CH,J0F12 AND 255		; ESC,TAB
        DEFB	03DH,J0F46 AND 255		; STOP
        DEFB	041H,J0F12 AND 255		; BS,SELECT,RETURN,SPACE
        DEFB	042H,J0F06 AND 255		; HOME
        DEFB	0FFH,J0F12 AND 255		; ins,del,left,up,down,right,numeric pad

        DEFS	01BABH-$,0	; unused space (1 byte)

; *************************************
; END OF RUSSIAN KEYBOARD HANDLER
; *************************************
